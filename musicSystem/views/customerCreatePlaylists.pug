extends layout
 
block content
    form#registerForm(method='POST' action='http://localhost:3000/musicSystem/customerCreatePlaylist')
        h1 #{msg}
        div
            input#creatorId(type='hidden',value=id ,name='creatorId')
        div
            label(for='pName') Playlist Name:
            input#pName(type='text',name='pName')       
        div
            button(type='submit') Create Playlist